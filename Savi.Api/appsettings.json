{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "CloudinarySettings": {
    "CloudName": "your_cloud_name",
    "ApiKey": "your_api_key",
    "ApiSecret": "your_api_secret"
  },
  "ConnectionStrings": {
    "SaviContext": "Data Source=.;Initial Catalog=SaviDbContext;Integrated Security=True;"

  },
  "EmailSettings": {
    "Host": "your_smtp_host",
    "Port": "your_smtp_port",
    "FromMail": "your_sender_email_address",
    "Password": "your_sender_email_password"
  },

  "AllowedHosts": "*"
}
